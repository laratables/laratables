{
    "name": "laratables/laratables",
    "description": "Ajax support of DataTables for Laravel 5.5+ to v12+",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Support",
            "email": "support@laratables.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Freshbitsweb\\Laratables\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\Laratables\\LaratablesServiceProvider"
            ]
        }
    }
}
